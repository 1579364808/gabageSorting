.box {
    border-bottom: rgb(230, 230, 230) solid 3rpx;
    height: 150rpx;
    display: flex;
    flex-direction: column;
    justify-content: space-evenly;

}

.top {
    display: flex;
    flex-direction: row;
    justify-content: space-around;
    font-size: 93%;
}

.bottom {
    display: flex;
    justify-content: space-around;
    color: rgb(99, 91, 91);
}

.bottom image {
    width: 25rpx;
    height: 25rpx;
}

#Page {
    position: relative;
    top: 50rpx;
    background-color: rgb(248, 248, 248);
    margin-left: 30rpx;
    margin-right: 30rpx;
    box-sizing: content-box;
}

.van-swipe-cell__right {
    display: inline-block;
    width: 65px;
    height: 44px;
    font-size: 15px;
